#!/bin/sh
exedir="$OPENSHIFT_HASKELL_DIR/usr/lib/ghc-7.6.3"
exeprog="ghc-pkg"
executablename="$exedir/$exeprog"
datadir="$OPENSHIFT_HASKELL_DIR/usr/share"
bindir="$OPENSHIFT_HASKELL_DIR/usr/bin"
topdir="$OPENSHIFT_HASKELL_DIR/usr/lib/ghc-7.6.3"
#!/bin/sh

PKGCONF="$topdir/package.conf.d"
exec "$executablename" --global-package-db "$PKGCONF" ${1+"$@"}

